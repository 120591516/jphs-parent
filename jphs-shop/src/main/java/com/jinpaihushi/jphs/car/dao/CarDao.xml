           <if test="creatorName!=null">
				C.creator_name=#{creatorName}, 
		   </if>
		   <if test="code!=null">
				C.code=#{code}, 
		   </if>
		</set>
	</sql>
	
	<sql id="orderby">
		<if test="orderby != null">
			ORDER BY ${orderby}
		</if>
