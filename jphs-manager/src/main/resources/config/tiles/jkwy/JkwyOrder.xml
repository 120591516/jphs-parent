			value="/WEB-INF/view/statistics/left.jsp" />
		<put-attribute type="template" name="body"
			value="/WEB-INF/view/statistics/jkwy/order/index.jsp" />
		<put-list-attribute name="css" inherit="true">
			<add-attribute value="/static/css/access/log/access.css" />					
		</put-list-attribute>
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute value="/static/js/analysis/weekSatatis.js" />
