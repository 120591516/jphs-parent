			value="/WEB-INF/view/jkwy/jkwy/order/list/index.jsp" />
		<put-attribute type="template" name="left"
			value="/WEB-INF/view/order/left.jsp" />
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute value="/static/js/jkwy/jkwy/order/list.js" />
		</put-list-attribute>
	</definition>
	
	<definition name="jkwy/jkwy/order/edit" extends="default.layout">
		<put-attribute type="string" name="title" value="编辑" />
