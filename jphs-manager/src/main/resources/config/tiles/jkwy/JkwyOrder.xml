			value="/WEB-INF/view/order/left.jsp" />
		<put-list-attribute name="scripts" inherit="true">
			<add-attribute value="/static/js/jkwy/jkwy/order/edit.js" />
		</put-list-attribute>
	</definition>
	
	<definition name="jkwy/jkwy/order/detail" extends="default.layout">
		<put-attribute type="string" name="title" value="详情" />
